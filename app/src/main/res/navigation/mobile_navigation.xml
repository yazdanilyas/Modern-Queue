    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.cybereast.virtualqueue.doctor.ui.fragments.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_more"
        android:name="com.cybereast.virtualqueue.doctor.ui.fragments.more.MoreFragment"
