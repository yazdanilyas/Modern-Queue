    app:startDestination="@+id/navigation_session">

    <fragment
        android:id="@+id/navigation_session"
        android:name="com.cybereast.virtualqueue.doctor.ui.fragments.sessions.SessionsFragment"
        android:label="@string/title_my_sessions"
